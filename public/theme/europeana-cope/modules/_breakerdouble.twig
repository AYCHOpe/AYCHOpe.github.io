{% set override_css_id = module.override_css_id %}
{% set override_css_class = module.override_css_class %}

<section {% if override_css_id %}id="{{ override_css_id }}"{% endif %} class="group items-grid breakerdouble {%- if override_css_class %} {{ override_css_class }}{% endif -%}">
{% for record in records %}
    {% if record.id is defined %}

        {% include "views/_breaker.twig" with { layoutclass : 'half' } %}

    {% endif %}
{% endfor %}
</section>



{
    "name": "europeana/widgets",
    "description": "Add a field for view widgets.",
    "type": "bolt-extension",
    "keywords": [
        "europeana",
        "widgets"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lodewijk Evers",
            "email": "lodewijk@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Europeana\\Widgets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Europeana\\Widgets\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Europeana\\Widgets\\WidgetsExtension",
        "bolt-icon": "bolt-icon.png",
        "bolt-screenshots": [
            "screenshots/screenshot1.png"
        ]
    }
}

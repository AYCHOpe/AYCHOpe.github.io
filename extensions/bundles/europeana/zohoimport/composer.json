{
    "name": "europeana/zohoimport",
    "description": "Import XML, ATOM or JSON nodes into a content type",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lodewijk Evers",
            "email": "lodewijk@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Europeana\\ZohoImport\\": [
                "src"
            ],
            "mjohnson\\utility\\": [
                "src/TypeConverter"
            ]
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Europeana\\ZohoImport\\ZohoImportExtension"
    }
}
